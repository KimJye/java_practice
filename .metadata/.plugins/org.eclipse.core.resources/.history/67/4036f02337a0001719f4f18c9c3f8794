
public class Person {
	Integer num1;
	String num2;
	
	public Person(Integer num1, String num2){
		this.num1 = num1;
		this.num2 =num2;
	}
	@Override
	public boolean equals(Object obj){
		if(obj instanceof Person==false)
			return false;
		Person p = (Person)obj;
		return (this.num1==null? p.num1==null : this.num2.equals(p.num2)&&this.num1==p.num1);
	}
	
}
